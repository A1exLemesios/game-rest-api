package game.api.Model;

public class PlayResponse {

	public enum GameResultStatus {
	    WIN,
	    LOSS, 
	    INVALID_INPUT; 
	}

	private String score;
	private GameResultStatus gameResult;
	
	public GameResultStatus getGameResult() {
		return gameResult;
	}

	public void setGameResult(GameResultStatus gameResult) {
		this.gameResult = gameResult;
	}

	public String getScore() {
		return score;
	}

	public void setScore(String score) {
		this.score = score;
	}
	
	

}
